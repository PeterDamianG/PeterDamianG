{
  "name": "@peterdg/template-next",
  "version": "0.1.1",
  "description": "A personal template for fast-start with next.",
  "keywords": [
    "nextjs",
    "APP",
    "next",
    "react",
    "template"
  ],
  "author": "Peter Damian GÃ³mez <PeterDamianG@Gmail.com> (https://github.com/PeterDamianG)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "export": "next build && next export",
    "see-export": "cd out && index.html",
    "dev": "next dev",
    "dev-debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "prettier": "prettier --write src/**/*.{js,jsx,css} && prettier --check src/**/*.{js,jsx,css}",
    "eslint": "eslint src/**/*.{js,jsx} --fix",
    "test": "jest --verbose --runInBand",
    "coverage": "jest --coverage --verbose --runInBand",
    "see-coverage": "cd coverage/lcov-report && index.html",
    "docs": "jsdoc -c jsdoc.json",
    "see-docs": "cd docs && index.html",
    "cypress": "cypress open",
    "cypress-headless": "cypress run"
  },
  "dependencies": {
    "embla-carousel": "^4.5.3",
    "next": "10.2.0",
    "next-themes": "^0.0.14",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-responsive": "^8.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "babel-jest": "^26.6.3",
    "clean-jsdoc-theme": "^3.2.7",
    "cypress": "^7.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.7",
    "jsdoc-http-plugin": "^0.3.2",
    "prettier": "^2.3.0"
  }
}
